      "key": {
        "uuid": "ef424f6d_3b12d3d1",
        "filename": "chrome/browser/extensions/extension_cookie_notifier.cc",
        "patchSetId": 9
      },
      "lineNbr": 53,
      "author": {
        "id": 1001809
