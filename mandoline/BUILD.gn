group("all") {
  # Meta-target, don't link into production code.
  testonly = true

  # TODO(beng): should be able to remove this in favor of explicit dependencies
  #             from subdirs.
  deps = [
    "//mandoline/ui",
    "//mojo",
  ]

  if (!is_component_build) {
    deps += [ "//mandoline/app" ]
  }
}
