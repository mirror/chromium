component("url_log") {
  sources = [
    "url_log.cc",
    "url_log.h",
    "url_log_export.h",
  ]

  defines = [ "URL_LOG_IMPLEMENTATION" ]

  deps = [
    "//base",
    "//url:url",
  ]
}

source_set("unit_tests") {
  testonly = true
  sources = [
    "url_log_unittest.cc",
  ]

  deps = [
    ":url_log",
    "//base",
    "//base/test:test_support",
    "//testing/gtest",
    "//url:url",
  ]
}
