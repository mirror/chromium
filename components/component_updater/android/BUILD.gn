import("//build/config/android/rules.gni")

android_library("componentupdater_java") {
  java_files = [
    "java/src/org/chromium/components/componentupdater/ApkSignatureSchemeV2Verifier.java",
    "java/src/org/chromium/components/componentupdater/ZipUtils.java",
  ]
  deps = [
    "//base:base_java",
  ]
}

generate_jni("jni_componentupdater_header") {
  sources = [
    "java/src/org/chromium/components/componentupdater/ApkSignatureSchemeV2Verifier.java",
  ]
  jni_package = "components"
}
