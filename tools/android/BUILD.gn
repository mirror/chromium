# Intermediate target grouping the android tools needed to run native
# unittests and instrumentation test apks.
#
# GYP: //android/tools/android_tools.gyp:android_tools
group("android_tools") {
  deps = [
    "//tools/android/forwarder2",
    "//tools/android/md5sum",
  ]
}
