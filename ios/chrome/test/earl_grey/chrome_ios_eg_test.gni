import("//ios/chrome/test/earl_grey/chrome_ios_base_eg_test.gni")

# Template wrapping chrome_ios_base_eg_test, setting default values for
# EarlGrey test based on //ios/chrome/app:chrome.
#
template("chrome_ios_eg_test") {
  chrome_ios_base_eg_test(target_name) {
    forward_variables_from(invoker, "*")
