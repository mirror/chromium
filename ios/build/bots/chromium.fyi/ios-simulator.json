{
  "comments": [
    "Runs tests on @3x, @2x, 64-bit, 32-bit, phone, tablet, iOS 10, and iOS 9."
  ],
  "xcode version": "8.0",
  "gn_args": [
    "additional_target_cpus=[\"x86\"]",
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "target_cpu=\"x64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "configuration": "Debug",
  "sdk": "iphonesimulator10.0",
  "tests": [
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s Plus",
      "os": "9.0",
      "xcode version": "8.0"
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPhone 6s",
      "os": "9.0",
      "xcode version": "8.0"
    },
    {
      "include": "common_tests.json",
      "device type": "iPhone 6s",
      "os": "9.0",
      "xcode version": "8.0"
    },
    {
      "include": "screen_size_dependent_tests.json",
      "device type": "iPad Retina",
      "os": "9.0",
      "xcode version": "8.0"
    },
    {
      "include": "common_tests.json",
      "device type": "iPad Retina",
      "os": "9.0",
      "xcode version": "8.0"
    }
  ]
}
