{
  "name": "webRequest permissions initiator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tests circumstances of exposing initiator in webRequest API.",
  "permissions": [
      "tabs",
      "webRequest",
      "*://*.example.com/*",
      "*://*.example2.com/*",
      "*://*.example3.com/*",
      "*://*.example4.com/*"],
  "background": {
    "scripts": ["test_initiator.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"]
    }
  ]
}
