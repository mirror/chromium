{
  "manifest_version": 2,
  "name": "History Override",
  "description": "Overrides the History Page",
  "version": "1.0",
  "chrome_url_overrides" : {
   "history": "history.html"
 },
  "browser_action": {
    "default_title": "History"
  },
  "permissions": [
    "activeTab",
    "history",
    "<all_urls>"
  ]
}
