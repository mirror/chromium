module chrome.mojom;

// Tells the renderer that it is displaying an interstitial page.
interface InterstitialPageRenderer {
  // Message sent from the browser to the render to inform the renderer
  // that it is displaying interstitial page to ignore the content settings
  SetAsInterstitial();
};
