{
  "manifest_version": 1,
  "name": "exe:content_browser",
  "process-group": "browser",
  "display_name": "Content Browser",
  "capabilities": {
    "required": {
      "*": { "interfaces": [ "*" ] },
      "mojo:shell": {
        "classes": [
          "shell:client_process",
          "shell:instance_name",
          "shell:user_id"
        ],
        "interfaces": [ "*" ]
      }
    }
  }
}
