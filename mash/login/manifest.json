{
  "manifest_version": 1,
  "name": "service:login",
  "display_name": "Login",
  "capabilities": {
    "provided": {
      "login:login": [ "mash::login::mojom::Login" ]
    },
    "required": {
      "*": { "classes": [ "app" ] },
      "service:mash_init": { "classes": [ "mash_init:init" ] },
      "service:ui": { "classes": [ "ui:user_access_manager" ] },
      "service:shell": { "classes": [ "shell:all_users" ] }
    }
  }
}
