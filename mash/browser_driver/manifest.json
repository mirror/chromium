{
  "manifest_version":  1,
  "name": "mojo:browser_driver",
  "display_name": "Browser Driver",
  "capabilities": {
    "required": {
      "*": {
        "classes": [ "mus:window_manager" ]
      },
      "exe:chrome": {
        "classes": [ "mash:launchable" ]
      },
      "mojo:catalog": {
        "interfaces": [ "catalog::mojom::Catalog" ]
      }
    }
  }
}
