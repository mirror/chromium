module viz.mojom;

import "mojo/common/unguessable_token.mojom";

struct LocalSurfaceId {
  // An identifier to uniquely identify a surface.
  // Prior to Viz, this is allocated by the child.
  // After Viz, this is allocated by the parent.
  uint32 parent_id;

  mojo.common.mojom.UnguessableToken nonce;
};

