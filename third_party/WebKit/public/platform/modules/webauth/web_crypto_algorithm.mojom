[JavaPackage="org.chromium.webauth.mojom"]
module webauth.mojom;

enum WebCryptoAlgorithmId {
  NONE,
  SHA_256,
  ECDSA,
};

enum WebCryptoNamedCurve {
  NONE,
  P256,
};

struct WebCryptoAlgorithmParams {
  WebCryptoNamedCurve named_curve;
};

struct WebCryptoAlgorithm {
  WebCryptoAlgorithmId id;
  WebCryptoAlgorithmParams param;
};