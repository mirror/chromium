module blink.mojom;

import "third_party/WebKit/public/platform/origin_manifest.mojom";
import "url/mojo/origin.mojom";

interface OriginManifestStore {
  Get(url.mojom.Origin origin) => (OriginManifest? om);
  GetOrFetch(url.mojom.Origin origin, string version) => (OriginManifest? om);
  Remove(url.mojom.Origin origin) => ();

  [Sync]
  GetCORSPreflight(url.mojom.Origin origin) => (CORSPreflight? corspreflight);
  [Sync]
  GetContentSecurityPolicies(url.mojom.Origin origin) => (array<ContentSecurityPolicy> csps);
};
