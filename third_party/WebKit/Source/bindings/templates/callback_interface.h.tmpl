namespace blink {

class {{v8_class}} final : public CallbackInterfaceBase {
 public:
  static {{v8_class}}* Create(v8::Local<v8::Object> callback_object) {
    return new {{v8_class}}(callback_object);
  }

  ~{{v8_class}}() override = default;

{% for method in methods %}
