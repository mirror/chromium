{% endfor %}

 private:
  {{v8_class}}(v8::Local<v8::Object> callback_object)
      : CallbackInterfaceBase(callback_object, {# one space after comma #}
                              {%- if is_single_operation_callback_interface -%}
                              kSingleOperation {%- else -%} kNotSingleOperation
                              {%- endif %}) {}
};

}  // namespace blink

