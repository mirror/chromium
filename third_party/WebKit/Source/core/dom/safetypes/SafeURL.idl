[
    Exposed=(Window,Worker),
    RuntimeEnabled=SafeHTMLTypes
] interface SafeURL {
    [CallWith=ScriptState] static Promise create(DOMString url);
    [CallWith=ScriptState] static Promise unsafelyCreate(DOMString url);

    stringifier;
};
