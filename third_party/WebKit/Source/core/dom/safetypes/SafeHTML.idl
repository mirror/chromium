[
    Exposed=(Window,Worker),
    RuntimeEnabled=SafeHTMLTypes
] interface SafeHTML {
    [CallWith=ScriptState] static Promise escape(DOMString html);
    [CallWith=ScriptState] static Promise unsafelyCreate(DOMString html);


    stringifier;
};

