// https://drafts.csswg.org/cssom/#the-elementcssinlinestyle-interface

[Exposed=Window,
 NoInterfaceObject]
interface ElementCSSInlineStyle {
  // style should have [PutForwards=cssText]
  [SameObject] readonly attribute CSSStyleDeclaration style;
};
