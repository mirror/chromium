dictionary MediaTrackCapabilities {
    // W3C Image Capture API
    // https://w3c.github.io/mediacapture-image/#mediatrackcapabilities-section
    // TODO(mcasas) move out when partial dictionaries are supported
    // http://crbug.com/579896.
    [OriginTrialEnabled=ImageCapture] sequence<DOMString>  whiteBalanceMode;
    [OriginTrialEnabled=ImageCapture] sequence<DOMString>  exposureMode;
    [OriginTrialEnabled=ImageCapture] sequence<DOMString>  focusMode;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   exposureCompensation;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   colorTemperature;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   iso;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   brightness;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   contrast;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   saturation;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   sharpness;
    [OriginTrialEnabled=ImageCapture] MediaSettingsRange   zoom;
    [OriginTrialEnabled=ImageCapture] boolean              torch;
};
