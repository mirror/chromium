mojom = "//third_party/WebKit/public/platform/modules/credentialmanager/credential_manager_v2.mojom"
public_headers = [
  "//third_party/WebKit/public/platform/WebCredentialMediationRequirement.h",
  "//third_party/WebKit/public/platform/WebCredentialManagerError.h",
  "//third_party/WebKit/Source/modules/credentialmanager/Credential.h",
  "//third_party/WebKit/Source/modules/credentialmanager/FederatedCredential.h",
  "//third_party/WebKit/Source/modules/credentialmanager/PasswordCredential.h",
]
traits_headers = [ "//third_party/WebKit/Source/modules/credentialmanager/CredentialManagerTraits.h" ]
