  "//third_party/WebKit/Source/modules/credentialmanager/CredentialManagerTraits.cpp",
]
deps = [
  "//mojo/public/cpp/bindings",
  "//third_party/WebKit/Source/core",
  "//third_party/WebKit/Source/modules/credentialmanager:credentialmanager_types",
  "//third_party/WebKit/Source/platform",
]
type_mappings = [
  "credential_manager.mojom.CredentialMediationRequirement=::blink::WebCredentialMediationRequirement",
  "credential_manager.mojom.CredentialManagerError=::blink::WebCredentialManagerError",
  "credential_manager.mojom.Credential=::blink::Persistent<::blink::Credential>[nullable_is_same_type]",
