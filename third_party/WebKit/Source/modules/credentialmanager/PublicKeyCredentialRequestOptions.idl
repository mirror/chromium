// https://w3c.github.io/webauthn/#typedefdef-authenticationextensions
typedef record<DOMString, DOMString>       AuthenticationExtensions;

// https://www.w3.org/TR/2017/WD-webauthn-20170505/#dictdef-publickeycredentialrequestoptions

dictionary PublicKeyCredentialRequestOptions {
    required BufferSource challenge;
    unsigned long timeout;
    USVString rpId;
    sequence <PublicKeyCredentialDescriptor> allowList = [];
    AuthenticationExtensions extensions;
};
