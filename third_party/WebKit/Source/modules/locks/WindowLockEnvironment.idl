// https://github.com/inexorabletash/web-locks
enum LockMode { "shared", "exclusive" };

// TODO(jsbell): SecureContext
[
    ImplementedAs=GlobalLockEnvironment
] partial interface Window {
    [CallWith=ScriptState, RaisesException] Promise<Lock> requestLock((DOMString or sequence<DOMString>) scope, LockMode mode, optional LockOptions options);
};
