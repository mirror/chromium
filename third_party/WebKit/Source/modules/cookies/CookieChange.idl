// Async Cookies API
// https://github.com/WICG/async-cookies-api
//
// OWP launch tracking bug
// https://crbug.com/729800

// Monitoring
// https://github.com/WICG/async-cookies-api/blob/gh-pages/explainer.md#monitoring

enum CookieChangeType {
  "hidden",
  "visible"
};

dictionary CookieChange {
  CookieChangeType type;
  DOMString name;
  DOMString? value;
  DOMString url;
  CookieStore cookieStore;
  double index;
  CookieList all;
};

typedef sequence<CookieChange> CookieChanges;
