# This config is applied to targets that depend on libvpx.
config("libvpx_external_config") {
  include_dirs = [ "//third_party/libvpx/source/libvpx" ]
}

group("libvpx_new") {
  public_deps = [
    "//third_party/libvpx",
  ]
  public_configs = [ ":libvpx_external_config" ]
}
